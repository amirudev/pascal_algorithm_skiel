Program lingkaran;
const
  Phi = 3.14;
var
  jari, luas, kel : real;
Begin
  jari := 10;
  luas := phi * jari * jari;
  kel := phi * jari * 2;
  writeln('HASIL');
  writeln('-----------');
  writeln('Jari-jari lingkaran', jari:6:2);
  writeln('Luas lingkaran ', luas:6:2);
  writeln('Keliling lingkaran', kel:6:2);
  Readln;
End.